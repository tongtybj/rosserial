<launch>
  <arg name="port" default="/dev/ttyUSB0" />
  <arg name="baud" default="921600" />
  <node pkg="rosserial_server" type="serial_node" name="rosserial_server" output="screen">
    <param name="port" value="$(arg port)" />
    <param name="baud" value="$(arg baud)" />
    <param name="spinal_uart_mode" value="true" />
  </node>
  <node pkg="rosserial_python" type="message_info_service.py" name="rosserial_message_info" />

  <node pkg="rosserial_server" type="rosservice_server_manager.py" name="" output="screen" />
</launch>
